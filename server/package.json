{
    "name": "cell-wall-server",
    "version": "2.0.0",
    "description": "Server for the CellWall",
    "license": "UNLICENSED",
    "author": "Tiger Oakes <contact@tigeroakes.com> (https://tigeroakes.com)",
    "repository": "NotWoods/cell-wall",
    "bugs": "https://github.com/NotWoods/cell-wall/issues",
    "homepage": "https://github.com/NotWoods/cell-wall#readme",
    "main": "index.js",
    "scripts": {
        "test": "jest --forceExit --coverage --verbose",
        "build": "tsc",
        "start": "node index.js",
        "lint:ts": "prettier --parser typescript \"src/**/*.ts\"",
        "lint:js": "prettier --parser babylon \"public/**/*.js\" \"index.js\"",
        "lint": "npm run lint:ts -- --write && npm run lint:js -- --write",
        "lint:check": "npm run build && npm run lint:ts -- --check && npm run lint:js -- --check"
    },
    "dependencies": {
        "appium-adb": "^7.2.0",
        "dotenv": "^6.2.0",
        "interactjs": "^1.3.4",
        "koa": "^2.7.0",
        "koa-static": "^5.0.0",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0"
    },
    "devDependencies": {
        "@types/dotenv": "^6.1.0",
        "@types/jest": "^23.3.13",
        "@types/koa": "^2.0.48",
        "@types/koa-joi-router": "^5.1.0",
        "@types/koa-static": "^4.0.0",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "jest": "^23.6.0",
        "prettier": "^1.16.3",
        "ts-jest": "^23.10.5",
        "typescript": "^3.2.4"
    },
    "engines": {
        "node": ">=10.14.2"
    },
    "jest": {
        "preset": "ts-jest"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "proseWrap": "always"
    }
}
