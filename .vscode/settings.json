{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": {
      "when": "$(basename)"
    }
  },
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/lib": true,
    "**/coverge": true,
    "package-lock.json": true
  },
  "typescript.referencesCodeLens.enabled": true,
  "python.pythonPath": "C:\\Program Files\\Python37\\python.exe"
}
